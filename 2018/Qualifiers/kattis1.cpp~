#include <iostream>
#include <cstdlib>
#include <cmath>
using namespace std;
int main() {
  int input;
  cin >> input;
  int i =1;
  while(pow(++i, 9) <= input) {
  }
  i--;
  bool flag = false;
  for(int j = i; j > 0 ; j--) {
    for(int k = 0; k < 9; k++) {
      if(input%j == 0){
        input = input/j;
        flag = true;
      }
      else {
        input = input/j;
        flag = false;
        break;
      }
    }
    if(flag) {
      cout << j;
      return 0;
    }
    
  }
  return 0;
}
  
  
  
  
